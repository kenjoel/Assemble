{{#partial "start"}}
    <div class="row">
        {{#if "squads"}}
        <div class="col-4">
            {{#each squads}}
                <ul>
                    <li>Name: {{name}}</li>
                    <li>Cause: {{cause}}</li>
                    <li>Size: {{size}}</li>
                </ul>
            {{/each}}
        </div>
        {{/if}}
    </div>

{{/partial}}

{{>layout.hbs}}